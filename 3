def find_max_min(arr):
    if len(arr) == 0:
        print("The array is empty.")
        return

    maximum = minimum = arr[0]

    for num in arr:
        if num > maximum:
            maximum = num
        if num < minimum:
            minimum = num

    print("Maximum is:", maximum)
    print("Minimum is:", minimum)

arr1 = [1, 2, 3, 4, 5]
find_max_min(arr1)

arr2 = [5, 3, 7, 4, 2]
find_max_min(arr2)
